<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.xuhen.lottery.activity.BaseSettingActivity" >

    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/iv_title_bg"
    	android:layout_width="1080px"
    	android:layout_height="122px"
    	android:layout_x="0px"
    	android:layout_y="0px"
    	android:background="@drawable/iv_title_bg"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/iv_title_user_bg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="0px"
    	android:layout_y="0px"
    	android:background="@drawable/iv_title_item_bg"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/iv_title_app_bg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="360px"
    	android:layout_y="0px"
    	android:background="@drawable/iv_title_item_bg"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/iv_title_setting_bg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/iv_title_item_bg"        
    />
    
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/tv_title_user"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="0px"
    	android:layout_y="0px"
	    android:text="账号管理"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="20sp" />
    
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/tv_title_app"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="360px"
    	android:layout_y="0px"
	    android:text="应用库"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="20sp" />
    
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/tv_title_setting"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
	    android:text="系统通用"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="37px" />
     <!-- 设置页面背景 -->
     <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_message_bg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_message_bg"        
    />
     <!-- 设置图标 -->
     <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_ico_bg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_ico_bg"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_system_ico"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_system_ico"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_network_ico"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_network_ico"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_other_ico"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_other_ico"        
    />
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_about_ico"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_about_ico"        
    />
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/setting_tv_setting_system"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
	    android:text="　产品信息"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="34px" />
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/setting_tv_setting_network"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
	    android:text="　WIFI连接"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="37px" />
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/setting_tv_setting_other"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
	    android:text="　其它设置"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="37px" />
  
    <com.xuhen.lottery.view.MyTextView 
        android:id="@+id/setting_tv_setting_about"
	   	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
	    android:text="　关于我们"
	    android:textColor="#fefffc"
	    android:gravity="center_vertical"
	    android:textAppearance="?android:attr/textAppearanceMedium"
	    android:textSize="37px" />
    
    <com.xuhen.lottery.view.MyImageView 
    	android:id="@+id/setting_iv_setting_other_msg"
    	android:layout_width="361px"
    	android:layout_height="120px"
    	android:layout_x="720px"
    	android:layout_y="0px"
    	android:background="@drawable/setting_iv_setting_other_msg"        
    />

</AbsoluteLayout>
